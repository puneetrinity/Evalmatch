  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "prebuild": "node scripts/inject-env.cjs",
    "build:client": "vite build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=build",
    "copy:migrations": "mkdir -p build/migrations && cp server/migrations/*.sql build/migrations/ && cp server/migrate.cjs build/",
    "build": "npm run build:client && npm run build:server && npm run copy:migrations",
    "start": "node build/index.js",
    "railway:start": "node build/index.js",
    "check": "tsc",
    "lint": "eslint server/ --max-warnings 0",
    "lint:fix": "eslint server/ --fix",
    "lint:check": "eslint server/",
    "test": "node --experimental-vm-modules --max_old_space_size=2048 --max-semi-space-size=256 node_modules/.bin/jest --config jest.config.mjs",
    "test:unit": "node --experimental-vm-modules --max_old_space_size=1536 --max-semi-space-size=128 node_modules/.bin/jest --config jest.config.unit.mjs --testPathPattern=\"scoring-constants|api-contracts|schema-validation\"",
    "test:unit:watch": "node --experimental-vm-modules --max_old_space_size=1024 --max-semi-space-size=128 node_modules/.bin/jest --config jest.config.unit.mjs --watch",
    "test:unit:coverage": "node --experimental-vm-modules --max_old_space_size=1024 --max-semi-space-size=128 node_modules/.bin/jest --config jest.config.unit.mjs --coverage",
    "test:integration": "node --experimental-vm-modules --max_old_space_size=2048 --max-semi-space-size=256 node_modules/.bin/jest --config jest.config.integration.mjs tests/api-integration.test.ts --runInBand",
    "test:integration:watch": "node --experimental-vm-modules --max_old_space_size=2048 --max-semi-space-size=256 node_modules/.bin/jest --config jest.config.integration.mjs tests/api-integration.test.ts --watch",
    "test:schema": "node --experimental-vm-modules --max_old_space_size=1024 --max-semi-space-size=128 node_modules/.bin/jest --config jest.config.integration.mjs tests/schema-validation.test.ts",
    "test:e2e": "node --experimental-vm-modules --max_old_space_size=3072 --max-semi-space-size=512 node_modules/.bin/jest tests/e2e-workflow.test.ts --no-coverage --runInBand --verbose --detectOpenHandles --forceExit",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:schema",
    "test:full": "npm run test:all && npm run test:e2e",
    "test:quick": "npm run test:unit",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:schema",
    "test:runner": "node scripts/test-runner.js",
    "test:runner:ci": "node scripts/test-runner.js --ci --skip-e2e",
    "test:runner:coverage": "node scripts/test-runner.js --coverage --skip-e2e",
    "test:memory-optimized": "node scripts/test-memory-optimized.js",
    "test:memory-optimized:coverage": "node scripts/test-memory-optimized.js --coverage",
    "test:security": "node --experimental-vm-modules --max_old_space_size=1536 --max-semi-space-size=128 node_modules/.bin/jest --config jest.config.integration.mjs tests/security/ --ci --runInBand",
    "test:performance": "node --experimental-vm-modules --max_old_space_size=2048 --max-semi-space-size=256 node_modules/.bin/jest --config jest.config.integration.mjs tests/performance/ --ci --runInBand",
    "test:load": "node --experimental-vm-modules --max_old_space_size=2048 --max-semi-space-size=256 node_modules/.bin/jest --config jest.config.integration.mjs tests/load/ --ci --runInBand",
    "test:unit:core": "node --experimental-vm-modules --max_old_space_size=1536 --max-semi-space-size=128 node_modules/.bin/jest --config jest.config.unit.mjs --testPathPattern='scoring-constants|api-contracts|schema-validation' --ci --coverage",
    "test:unit:business": "node --experimental-vm-modules --max_old_space_size=1536 --max-semi-space-size=128 node_modules/.bin/jest --config jest.config.unit.mjs --testPathPattern='regex-patterns|groq-extraction|enhanced-experience-extraction' --ci --coverage",
    "test:unit:ui": "node --experimental-vm-modules --max_old_space_size=1536 --max-semi-space-size=128 node_modules/.bin/jest --config jest.config.unit.mjs --testPathPattern='components|hooks|utils' --ci --coverage",
    "test:memory-optimized:unit": "node scripts/test-memory-optimized.js --batch unit",
    "test:memory-optimized:integration": "node scripts/test-memory-optimized.js --batch integration,integration-large",
    "test:monitor": "node scripts/monitor-test-memory.js",
    "test:monitor:unit": "node scripts/monitor-test-memory.js --config jest.config.unit.mjs",
    "test:railway": "jest --config jest.config.integration.mjs tests/railway-deployment.test.ts",
    "test:railway:setup": "node scripts/railway-test-setup.js",
    "test:railway:full": "npm run test:railway:setup && npm run test:railway",
    "monitor:railway": "node scripts/railway-monitor.js",
    "monitor:performance": "node scripts/performance-monitor.js",
    "validate:config": "node -e \"console.log('Config validation not implemented yet')\"",
    "validate:schema": "npm run test:schema",
    "predeploy": "npm run test:quick && npm run validate:schema",
    "railway:test:predeploy": "npm run test:railway:full",
    "test:e2e:ui": "playwright test --ui",
    "test:comprehensive": "node scripts/comprehensive-test-runner.js",
    "test:comprehensive:verbose": "node scripts/comprehensive-test-runner.js --verbose",
    "test:comprehensive:ci": "node scripts/comprehensive-test-runner.js --skip-e2e --skip-load",
    "test:comprehensive:quick": "node scripts/comprehensive-test-runner.js --skip-e2e --skip-load --no-coverage",
    "test:security": "node --experimental-vm-modules --max_old_space_size=2048 --max-semi-space-size=256 node_modules/.bin/jest --config jest.config.integration.mjs tests/security/ --runInBand",
    "test:performance": "node --experimental-vm-modules --max_old_space_size=4096 --max-semi-space-size=512 node_modules/.bin/jest --config jest.config.integration.mjs tests/performance/ --runInBand --no-coverage",
    "test:load": "node --experimental-vm-modules --max_old_space_size=6144 --max-semi-space-size=1024 node_modules/.bin/jest --config jest.config.integration.mjs tests/load/ --runInBand --no-coverage",
    "test:e2e:full": "npx playwright test tests/e2e/",
    "test:integration:comprehensive": "node --experimental-vm-modules --max_old_space_size=3072 --max-semi-space-size=512 node_modules/.bin/jest tests/integration/ --runInBand",
    "test:auth": "node --experimental-vm-modules --max_old_space_size=2048 --max-semi-space-size=256 node_modules/.bin/jest tests/integration/firebase-auth-integration.test.ts --runInBand",
    "test:api": "node --experimental-vm-modules --max_old_space_size=3072 --max-semi-space-size=512 node_modules/.bin/jest tests/integration/comprehensive-api-integration.test.ts --runInBand",
    "db:push": "drizzle-kit push",
    "railway:deploy": "railway up",
    "docs:generate": "node scripts/generate-swagger-docs.js",
    "docs:validate": "node scripts/generate-swagger-docs.js"
  },
